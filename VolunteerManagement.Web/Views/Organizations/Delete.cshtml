@model VolunteerManagement.Domain.Entities.Organization

<h1>¿Estás seguro de que quieres eliminar esta Organizacion?</h1>
<p>Nombre: @Model.Name</p>
<p>Direcion: @Model.Address</p>

<form asp-action="DeleteConfirmed" asp-route-id="@Model.Id" method="post">
    <button type="submit" class="btn btn-danger">Sí, eliminar</button>
    <a href="@Url.Action("Index", "Organizations")" class="btn btn-secondary">Cancelar</a>
</form>
